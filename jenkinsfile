pipeline {
    agent any

    stages {
        stage ('Start') {
            steps {
                echo 'Iniciando a pipeline.'
            }
        }
        stage ('Test') {
            steps {
                & "c:\Users\victor\OneDrive - Fiap-Faculdade de Informática e Administração Paulista\Área de Trabalho\schneiderstream\mvnw.cmd" test -f "c:\Users\victor\OneDrive - Fiap-Faculdade de Informática e Administração Paulista\Área de Trabalho\schneiderstream\pom.xml"
            }
        }
         stage ('Build') {
            steps {
                & "c:\Users\victor\OneDrive - Fiap-Faculdade de Informática e Administração Paulista\Área de Trabalho\schneiderstream\mvnw.cmd" package -f "c:\Users\victor\OneDrive - Fiap-Faculdade de Informática e Administração Paulista\Área de Trabalho\schneiderstream\pom.xml"
            }
        }
         stage ('Upload') {
            steps {
                aws s3 cp "C:\Users\victor\OneDrive - Fiap-Faculdade de Informática e Administração Paulista\Área de Trabalho\schneiderstream\target\schneiderstream-0.0.1-SNAPSHOT.jar" s3://schneiderstreambucket/schneiderstream-0.0.1-SNAPSHOT.jar
            }
        }
    }
}